// envmaptint_fix
"sdk_lightmappedgeneric"
{
	"$basetexture" "plastic/plasticwall002a"
	"%keywords" "portal"

	// NOTE: Even though env_cubemap is here, it will turned off for DX6
	// because $multipass 1 isn't defined
	"$envmap" "env_cubemap"
	"$envmaptint" "[ .03 .03 .03 ]"
	"$envmapcontrast" 0.5
	"$envmapsaturation" 1

		"$bumpmap" "plastic/plasticwall001a_normal"

}
